date {\r\n    bottom: 13.5%;\r\n  }\r\n\r\n  &__name {\r\n    left: 6.5%;\r\n  }\r\n\r\n  &__exp-date {\r\n    right: 6.5%;\r\n  }\r\n\r\n  &__img-back-box {\r\n    margin-top: 2.4rem;\r\n    margin-left: 8rem;\r\n  }\r\n\r\n  &__cvc {\r\n    right: 12.5%;\r\n    top: 45%;\r\n  }\r\n\r\n  &__form {\r\n    width: 40rem;\r\n  }\r\n\r\n  &__form-group-exp-cvc {\r\n    @include flex(row, intial, center, 2.4rem);\r\n    margin-bottom: 2rem;\r\n  }\r\n\r\n  &__form-label {\r\n    display: block;\r\n    font-size: 1.4rem;\r\n    text-transform: uppercase;\r\n    margin-bottom: 0.8rem;\r\n    letter-spacing: 1.5px;\r\n    font-weight: 500;\r\n    color: var(--clr-very-dark-violet);\r\n  }\r\n\r\n  ::placeholder,\r\n  ::-moz-placeholder,\r\n  :-moz-placeholder,\r\n  ::-webkit-input-placeholder {\r\n    color: var(--clr-dark-violet);\r\n    font-size: 1.6rem;\r\n    font-weight: 500;\r\n    letter-spacing: 1.5px;\r\n  }\r\n\r\n  &__form-input {\r\n    border: 2px solid var(--clr-light-violet);\r\n    border-radius: 7.5px;\r\n    width: 100%;\r\n    padding: 1.2rem 1.6rem;\r\n    font-size: 1.6rem;\r\n    color: var(--clr-very-dark-violet);\r\n    font-weight: 500;\r\n    letter-spacing: 1.5px;\r\n    margin-bottom: 2.4rem;\r\n    outline: none;\r\n\r\n    &:focus {\r\n      border-color: transparent;\r\n      background-image: linear-gradient(var(--clr-white), var(--clr-white)),\r\n        linear-gradient(\r\n          to right,\r\n          var(--clr-blue-gradient),\r\n          var(--clr-purple-gradient)\r\n        );\r\n      background-origin: border-box;\r\n      background-clip: padding-box, border-box;\r\n    }\r\n  }\r\n  &__form-group {\r\n    position: relative;\r\n  }\r\n\r\n  &__error &__form-input {\r\n    &:focus {\r\n      border-color: var(--clr-red);\r\n    }\r\n  }\r\n\r\n  &__error-msg {\r\n    color: var(--clr-red);\r\n    position: absolute;\r\n    left: 0;\r\n    bottom: 0;\r\n    font-size: 1.2rem;\r\n    font-weight: 500;\r\n  }\r\n\r\n  input[type=\"number\"] {\r\n    -moz-appearance: textfield;\r\n    &::-webkit-inner-spin-button,\r\n    &::-webkit-outer-spin-button {\r\n      -webkit-appearance: none;\r\n      appearance: none;\r\n      margin: 0;\r\n    }\r\n  }\r\n\r\n  &__inputs-exp {\r\n    @include flex(row, intial, center, 1.2rem);\r\n  }\r\n\r\n  &__form-group-cvc {\r\n    width: 50rem;\r\n  }\r\n\r\n  &__form-submit-button {\r\n    width: 100%;\r\n    padding: 1.6rem;\r\n    background-color: var(--clr-very-dark-violet);\r\n    border: none;\r\n    border-radius: 7.5px;\r\n    color: var(--clr-light-violet);\r\n    font-size: 1.4rem;\r\n    font-weight: 500;\r\n    letter-spacing: 1.5px;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"1e2e9a60160f72cb50758eda50b3e631","cacheData":{"env":{}}}